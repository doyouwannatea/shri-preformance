{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "import-quotes"
    ],
    "rules": {
        "import-quotes/import-quotes": [1, "single"],
        "semi": ["warn", "never"]
    }
}